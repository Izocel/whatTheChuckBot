  "scripts": {
    "build": "rm -rf dist && tsc && cp -r src/api/certs dist/api/",
    "start": "npm install && npm run build && npm run sentry:ci && node ./dist/index.js",
    "nodeDist": "node ./dist/index.ts",
    "start:dev": "ts-node-dev ./src/index.ts",
    "start:prod": "node ./dist/index.js",
    "start:min": "node ./distMin/index.js",
    "uglify": "node ./gulp/main.js",
    "watch": "tsc -w",
    "sentry:ci": "sentry-cli sourcemaps inject --org rvdprojects --project what-the-chuck ./dist && sentry-cli sourcemaps upload --org rvdprojects --project what-the-chuck ./dist"
  },
  "dependencies": {
